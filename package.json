{
  "name": "my-project",
  "description": "Website template for Payload",
  "private": true,
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "engines": {
    "node": "20.x",
    "yarn": "4.1.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NEXT_BUILD=true node dist/server.js",
    "build": "cross-env NODE_ENV=production yarn build:payload && yarn build:server && yarn copyfiles && yarn build:next",
    "start": "yarn run serve",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NODE_ENV=production node --trace-warnings dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,js}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:graphQLSchema",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "lint:next": "next lint",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@next/third-parties": "^14.1.0",
    "@nouance/payload-better-fields-plugin": "^1.3.5",
    "@payloadcms/bundler-webpack": "^1.0.6",
    "@payloadcms/db-mongodb": "^1.4.3",
    "@payloadcms/plugin-nested-docs": "^1.0.12",
    "@payloadcms/plugin-seo": "^2.2.1",
    "@payloadcms/richtext-slate": "^1.4.0",
    "antd": "^5.14.2",
    "axios": "^1.6.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "escape-html": "^1.0.3",
    "eslint-config-next": "^14.1.0",
    "express": "^4.18.2",
    "fast-levenshtein": "^3.0.0",
    "flatted": "^3.3.1",
    "formik": "^2.4.5",
    "graphql-request": "^6.1.0",
    "gsap": "^3.12.5",
    "hash-it": "^6.0.0",
    "next": "^14.1.0",
    "next-i18next": "^15.2.0",
    "node-cache": "^5.1.2",
    "payload": "^2.11.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "6.22.1",
    "react-singleton-hook": "^4.0.1",
    "reset-css-complete": "^2.0.0",
    "sharp": "^0.33.2",
    "slugify": "^1.6.6",
    "string-width": "^7.1.0",
    "swiper": "^11.0.6",
    "swr": "^2.2.5",
    "usehooks-ts": "^2.15.1",
    "viewport-size": "^0.0.1",
    "yup": "^1.3.3",
    "yup-phone-lite": "^2.0.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.0",
    "@payloadcms/eslint-config": "^1.1.1",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.4.2",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/facebook-pixel": "^0.0.30",
    "@types/google.maps": "^3.55.3",
    "@types/gsap": "^3.0.0",
    "@types/node": "20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "copyfiles": "^2.4.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "slate": "0.102.0",
    "source-map-explorer": "^2.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "yarn@4.1.0"
}
